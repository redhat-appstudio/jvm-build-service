{
  "name": "patternfly-seed",
  "version": "0.0.2",
  "description": "An open source build scaffolding utility for web apps.",
  "repository": "https://github.com/patternfly/patternfly-react-seed.git",
  "homepage": "https://patternfly-react-seed.surge.sh",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.11",
    "@types/victory": "^35.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copy-webpack-plugin": "^12.0.1",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "imagemin": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.0",
    "openapi-typescript": "^7.0.0",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^6.23.1",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.0",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^6.0.0"
  },
  "dependencies": {
    "@patternfly/react-charts": "^7.3.1",
    "@patternfly/react-core": "^5.3.1",
    "@patternfly/react-icons": "^5.2.0",
    "@patternfly/react-styles": "^5.1.2",
    "@patternfly/react-table": "^6.0.0",
    "react": "^18",
    "react-dom": "^18",
    "sirv-cli": "^2.0.2"
  }
}
